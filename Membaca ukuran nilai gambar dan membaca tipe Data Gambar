import cv2 ## 2

img = cv2.imread("nature.jpg", cv2.IMREAD_COLOR)


cv2.namedWindow('Gambar alam', cv2.WINDOW_NORMAL)
cv2.imshow('Gambar alam', img)

cv2.waitKey(0)
cv2.destroyAllWindows()
